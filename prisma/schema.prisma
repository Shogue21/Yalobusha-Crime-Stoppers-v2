// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Tip {
  id String @id @default(cuid())
  description String 
  offenseType String
  address String
  nearestIntersection String
  neighborhood String
  references String
}